'use strict';

/* Services */

angular.module('HNServices', ['ngResource']).
  factory('Link', function($resource) {
  return $resource('http://api.ihackernews.com/page',
                   {
                     format: 'jsonp',
                     callback: 'JSON_CALLBACK'
                   },
                   {
                     query: {
                       method: 'JSONP'
                     }
                   });
});

